{
  "name": "nunjucks-prettier",
  "displayName": "Nunjucks-Prettier",
  "description": "nunjucks prettier formatter",
  "version": "0.0.1",
  "publisher": "guapibai",
  "repository": {
    "type": "git",
    "url": "https://github.com/hohogpb/vscode-nunjucks-formatter-prettier.git"
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Formatters"
  ],
  "icon": "icon.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "nunjucks",
        "aliases": [
          "HTML Nunjucks",
          "Nunjucks",
          "nunjucks",
          "Njk",
          "njk"
        ],
        "extensions": [
          ".nunjucks",
          ".nunjs",
          ".nunj",
          ".nj",
          ".njk",
          ".html",
          ".htm",
          "template",
          ".tmpl",
          ".tpl"
        ]
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "prettier": "2.8.8"
  }
}
